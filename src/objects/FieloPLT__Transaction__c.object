    <validationRules>
        <fullName>FieloPLT__ValidateDateTime</fullName>
        <active>true</active>
        <description>The Date &amp; Time for the Transaction must have a value</description>
        <errorConditionFormula>ISNULL(FieloPLT__Date__c)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Date__c</errorDisplayField>
        <errorMessage>The Date &amp; Time for the Transaction must have a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ValidateProcessedPoints</fullName>
        <active>true</active>
        <description>A processed Transaction can not have null points.</description>
        <errorConditionFormula>FieloPLT__Processed__c &amp;&amp; ISNULL(FieloPLT__Points__c)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Points__c</errorDisplayField>
        <errorMessage>A processed Transaction can not have null points</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ValidateTransactionType</fullName>
        <active>true</active>
        <errorConditionFormula>FieloPLT__Value__c != 0 &amp;&amp; ( ISPICKVAL(FieloPLT__Type__c, &apos;Code Insert&apos; ) || ISPICKVAL(FieloPLT__Type__c, &apos;Referral - Step 1&apos; ) || ISPICKVAL(FieloPLT__Type__c, &apos;Referral - Step 2&apos; ) || ISPICKVAL(FieloPLT__Type__c, &apos;Registration&apos; ) )</errorConditionFormula>
        <errorMessage>You must enter &quot;Purchase value = 0&quot; for transactions of types: Registration, Code insert, Referral - Simple signup, and Referral - Complete signup.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__ValidateTransactionValue</fullName>
        <active>false</active>
        <errorConditionFormula>FieloPLT__Value__c &lt;= 0 &amp;&amp; ISPICKVAL(FieloPLT__Type__c, &apos;Purchase&apos; )</errorConditionFormula>
        <errorDisplayField>FieloPLT__Value__c</errorDisplayField>
        <errorMessage>Value cannot be equal or less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FieloPLT__Validate_Member</fullName>
        <active>true</active>
        <errorConditionFormula>(FieloPLT__Member__c == null)</errorConditionFormula>
        <errorDisplayField>FieloPLT__Member__c</errorDisplayField>
        <errorMessage>You must choose a Member in the Member Field.</errorMessage>
    </validationRules>
